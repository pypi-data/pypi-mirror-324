{
  "application": "{{ .SpinnakerApp }}",
  "name": "{{ .PipelineName }}",
  "keepWaitingPipelines": false,
  "limitConcurrent": true,
  "parallel": true,
  "stages": [
    {
      "application": "{{ .SpinnakerApp }}",
      "cloudProvider": "titus",
      "cloudProviderType": "titus",
      "cluster": "{{ .SpinnakerApp }}",
      "credentials": "titustestvpc",
      "moniker": {
        "app": "{{ .SpinnakerApp }}",
        "cluster": "{{ .SpinnakerApp }}"
      },
      "name": "Find Image",
      "onlyEnabled": true,
      "refId": "find_image_stage",
      "regions": [
        "us-east-1"
      ],
      "selectionStrategy": "NEWEST",
      "target": "current_asg_dynamic",
      "type": "findImage"
    },
    {
      "clusters": [
        {
          "account": "titusprodvpc",
          "application": "{{.SpinnakerApp}}",
          "capacity": {
            "desired": 1,
            "max": 1,
            "min": 1
          },
          "capacityGroup": "{{.SpinnakerApp}}",
          "cloudProvider": "titus",
          "constraints": {
            "hard": {},
            "soft": {}
          },
          "delayBeforeDisableSec": 0,
          "delayBeforeScaleDownSec": 0,
          "env": {},
          "inService": true,
          "labels": {},
          "maxRemainingAsgs": 2,
          "migrationPolicy": {
            "type": "systemDefault"
          },
          "network": "default",
          "networkMode": "Ipv6AndIpv4",
          "provider": "titus",
          "region": "us-east-1",
          "registry": "prodregistry",
          "resources": {
            "allocateIpAddress": true,
            "cpu": 2,
            "disk": 10000,
            "memory": 2048,
            "networkMbps": 128
          },
          "rollback": {
            "onFailure": true
          },
          "scaleDown": true,
          "securityGroups": [],
          "serviceJobProcesses": {
            "disableDecreaseDesired": false,
            "disableIncreaseDesired": false
          },
          "strategy": "redblack",
          "targetGroups": [],
          "targetHealthyDeployPercentage": 100
        }
      ],
      "name": "Deploy",
      "refId": "deploy_stage",
      "requisiteStageRefIds": [
        "find_image_stage"
      ],
      "type": "deploy"
    }
  ],
  "triggers": []
}