{
    "name": "dash_deckgl",
    "version": "0.9.1",
    "description": "Deck.gl component for Dash",
    "repository": {
        "type": "git",
        "url": "git://github.com/oceanum-io/dash-deckgl.git"
    },
    "bugs": {
        "url": "https://github.com/oceanum-io/dash-deckgl/issues"
    },
    "homepage": "https://github.com/oceanum-io/dash-deckgl",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack serve --config ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js": "webpack --mode production",
        "build:backends": "dash-generate-components ./src/lib/components dash_deckgl -p package-info.json --r-prefix '' --jl-prefix '' --ignore 'utils|Deckgl.jsx'",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
        "build": "npm run build:js && npm run build:backends",
        "build:activated": "npm run build:js && npm run build:backends-activated"
    },
    "author": "Oceanum developers <developers@oceanum.science>",
    "license": "MIT",
    "dependencies": {
        "@deck.gl/aggregation-layers": "^9.1.0",
        "@deck.gl/carto": "^9.1.0",
        "@deck.gl/core": "^9.1.0",
        "@deck.gl/geo-layers": "^9.1.0",
        "@deck.gl/google-maps": "^9.1.0",
        "@deck.gl/json": "^9.1.0",
        "@deck.gl/jupyter-widget": "^9.1.0",
        "@deck.gl/layers": "^9.1.0",
        "@deck.gl/mesh-layers": "^9.1.0",
        "@deck.gl/react": "^9.1.0",
        "@loaders.gl/core": "^4.3.3",
        "@luma.gl/constants": "^9.1.0",
        "process": "^0.11.10",
        "ramda": "^0.30.1",
        "react-map-gl": "^7.1.9"
    },
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/preset-env": "^7.26.7",
        "@babel/preset-react": "^7.26.3",
        "@plotly/dash-component-plugins": "^1.2.3",
        "@plotly/webpack-dash-dynamic-import": "^1.3.0",
        "ajv": "^8.17.1",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^9.2.1",
        "copyfiles": "^2.4.1",
        "css-loader": "^7.1.2",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react": "^7.37.4",
        "prop-types": "^15.8.1",
        "react": "^19.0.0",
        "react-docgen": "^5.4.3",
        "react-dom": "^19.0.0",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "styled-jsx": "^5.1.6",
        "terser-webpack-plugin": "^5.3.11",
        "webpack": "5.97.1",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "^5.2.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
