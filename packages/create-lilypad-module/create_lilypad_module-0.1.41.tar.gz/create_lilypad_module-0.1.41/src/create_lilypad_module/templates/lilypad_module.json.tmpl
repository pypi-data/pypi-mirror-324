{
    "machine": {
        "gpu": 0,
        "cpu": 1000,
        "ram": 4000
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "0001-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": { "Concurrency": 1 },
            "Docker": {
                "WorkingDirectory": "/workspace",
                "Entrypoint": ["python", "/workspace/src/run_inference.py"],
                "EnvironmentVariables": [
                    "INPUT={{ js .input }}"
                ],
                "Image": "",
            },
            "Engine": "Docker",
            "Network": { "Type": "None" },
            "Outputs": [
                { "Name": "outputs", "Path": "/outputs" }
            ],
            "PublisherSpec": { "Type": "ipfs" },
            "Resources": { "CPU": "1", "Memory": "4000" },
            "Timeout": 600,
            "Wasm": { "EntryModule": {} },
        },
    },
}