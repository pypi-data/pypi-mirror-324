from matplotlib.tri.triangulation import Triangulation as Triangulation
from typing import Any

class TriRefiner:
    def __init__(self, triangulation) -> None: ...

class UniformTriRefiner(TriRefiner):
    def __init__(self, triangulation) -> None: ...
    def refine_triangulation(self, return_tri_index: bool = ..., subdiv: int = ...): ...
    def refine_field(self, z, triinterpolator: Any | None = ..., subdiv: int = ...): ...
