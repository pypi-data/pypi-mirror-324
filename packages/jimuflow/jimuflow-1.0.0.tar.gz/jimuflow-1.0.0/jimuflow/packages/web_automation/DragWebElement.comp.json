      "type": "number",
      "direction": "IN",
      "uiConfig": {
        "label": "Target Element x offset",
        "inputType": "expression",
        "dependsOn": {
          "variableName": "targetOffset",
          "operator": "==",
          "value": true
        }
      }
    },
    {
      "name": "targetY",
      "type": "number",
      "direction": "IN",
      "uiConfig": {
        "label": "Target Element y offset",
        "inputType": "expression",
        "dependsOn": {
          "variableName": "targetOffset",
          "operator": "==",
          "value": true
        }
      }
    },
    {
      "name": "dragOffsetX",
      "type": "number",
      "direction": "IN",
      "uiConfig": {
        "label": "Drag by x offset",
        "inputType": "expression",
        "dependsOn": {
          "variableName": "dragType",
          "operator": "==",
          "value": "by_offset"
        }
      }
    },
    {
      "name": "dragOffsetY",
      "type": "number",
      "direction": "IN",
