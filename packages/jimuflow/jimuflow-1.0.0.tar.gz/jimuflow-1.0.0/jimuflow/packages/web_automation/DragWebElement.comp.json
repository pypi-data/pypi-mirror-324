        "label": "Drag speed",
        "group": "advanced",
        "inputType": "combo_box",
        "required": false,
        "options": [
          {
            "value": "instant",
            "label": "Instant"
          },
          {
            "value": "fast",
            "label": "Fast"
          },
          {
            "value": "medium",
            "label": "Medium"
          },
          {
            "value": "slow",
            "label": "Slow"
          }
        ]
      }
    },
    {
      "name": "waitTime",
      "type": "number",
      "direction": "IN",
      "defaultValue": "\"30\"",
      "uiConfig": {
        "label": "Wait time(seconds)",
        "group": "advanced",
        "inputType": "expression",
        "required": false
      }
    }
