{
  "name": "WebDownload",
  "displayName": "Web download",
  "controlFlowType": "NORMAL",
  "type": "jimuflow.components.web_automation.WebDownloadComponent",
  "supportsErrorHandling": true,
  "primaryCategory": "Web Automation",
  "secondaryCategory": "Web Dialog Operation",
  "sortNo": 7,
  "helpUrl": "commands/WebAutomation/WebDialogOperation/web_download/",
  "variables": [
    {
      "name": "webPage",
      "type": "WebPage",
      "direction": "IN",
      "uiConfig": {
        "label": "Web page",
        "inputType": "expression"
      }
    },
    {
      "name": "downloadType",
      "type": "text",
      "direction": "IN",
      "defaultValue": "click_element",
      "uiConfig": {
        "label": "Download type",
        "inputType": "combo_box",
        "options": [
          {
            "value": "click_element",
            "label": "Click download button"
          },
          {
            "value": "open_url",
            "label": "Open download url"
          }
        ]
      }
    },
    {
      "name": "elementUri",
      "type": "text",
      "direction": "IN",
      "uiConfig": {
        "label": "Download button element",
        "inputType": "custom",
        "inputEditorType": "jimuflow.gui.components.web_element_selector.WebElementEdit",
        "dependsOn": {
          "variableName": "downloadType",
          "operator": "==",
          "value": "click_element"
        }
      }
    },
    {
      "name": "url",
      "type": "text",
      "direction": "IN",
      "uiConfig": {
        "label": "Download url",
        "inputType": "expression",
        "dependsOn": {
          "variableName": "downloadType",
          "operator": "==",
          "value": "open_url"
        }
      }
    },
    {
      "name": "saveFolder",
      "type": "text",
      "direction": "IN",
      "uiConfig": {
        "label": "Save folder",
        "inputType": "custom",
        "inputEditorType": "jimuflow.gui.components.file_path_editor.FolderPathEdit",
        "inputValueType": "expression"
      }
    },
    {
      "name": "fileNamingType",
      "type": "text",
      "direction": "IN",
      "defaultValue": "suggested",
      "uiConfig": {
        "label": "File naming type",
        "inputType": "combo_box",
        "options": [
          {
            "value": "suggested",
            "label": "Use suggested filename"
          },
          {
            "value": "custom",
            "label": "Custom filename"
          }
        ]
      }
    },
    {
      "name": "customFilename",
      "type": "text",
      "direction": "IN",
      "uiConfig": {
        "label": "Custom filename",
        "inputType": "expression",
        "dependsOn": {
          "variableName": "fileNamingType",
          "operator": "==",
          "value": "custom"
        }
      }
    },
    {
      "name": "overrideExistingFile",
      "type": "bool",
      "direction": "IN",
      "defaultValue": false,
      "uiConfig": {
        "label": "Override existing file",
        "inputType": "check_box",
        "required": false
      }
    },
    {
      "name": "downloadTimeout",
      "type": "number",
      "direction": "IN",
      "defaultValue": "\"300\"",
      "uiConfig": {
        "label": "Download timeout(seconds)",
        "inputType": "expression",
        "required": false
      }
    },
    {
      "name": "waitTime",
      "type": "number",
      "direction": "IN",
      "defaultValue": "\"30\"",
      "uiConfig": {
        "label": "Wait time(seconds)",
        "group": "advanced",
        "inputType": "expression",
        "required": false
      }
    },
    {
      "name": "downloadFilename",
      "type": "text",
      "direction": "OUT",
      "uiConfig": {
        "label": "Download filename"
      }
    }
  ]
}
