<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <script
            type="module"
            src="{{ request.app.url_path_for('static', path='rapidoc-min.js') }}"
        ></script>
    </head>
    <body>
        <rapi-doc
            spec-url="schema"
            theme="light"
            primary-color="#0a5175"
            load-fonts="false"
            render-style="view"
            show-header="false"
            allow-authentication="false"
            allow-spec-url-load="false"
            allow-spec-file-load="false"
        >
        </rapi-doc>
    </body>
</html>